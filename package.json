{
  "name": "fake-api-server",
  "version": "0.2.0",
  "description": "Fake api server mock",
  "license": "MIT",
  "author": "Reachfive <jeremy@reachfive.co>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ReachFive/fake-smtp-server"
  },
  "bugs": {
    "url": "https://github.com/ReachFive/fake-smtp-server/issues"
  },
  "homepage": "https://github.com/ReachFive/fake-smtp-server#readme",
  "main": "dist",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "scripts": {
    "build": "babel src -s -D -d dist ",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "bin": {
    "fake-api-server": "bin/fake-api-server"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "cors": "2.8.5",
    "express": "4.16.4",
    "express-validator": "5.3.1",
    "morgan": "1.9.1",
    "resource-router-middleware": "0.7.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "eslint": "5.14.1"
  }
}
